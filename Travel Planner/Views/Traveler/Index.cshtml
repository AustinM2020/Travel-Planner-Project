@model Travel_Planner.Models.Traveler

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #map {
        height: 600px;
        width: 600px;
    }
</style>
<h1>Index</h1>
<h2>Map</h2>
<div id="map"></div>
<script>
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: @Model.Lat, lng: @Model.Long },
            zoom: 8
        });
    }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=@APIkeys.googleApi&callback=initMap"
        async defer></script>



